    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Row</jsScript_name>
        <jsScript_script>var chartTypeName = ownerTypeName+'';
var ownerTypeName = ownerTypeName.charAt(0).toUpperCase() + ownerTypeName.substr(1);
var cdeTypeName   = "ccc" + ownerTypeName;

var ownerTypeDescription = '';
if(customChartTitleMap) {
    if(customChartTitleMap.hasOwnProperty(chartTypeName)) {
	    ownerTypeDescription = customChartTitleMap[chartTypeName];
    }
}

if(ownerTypeDescription=='') {
	ownerTypeDescription = "CCC " + ownerTypeName.replace(/([a-z])([A-Z])/g, "$1 $2");
}
</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>1</jsScript_type>
        <jsScript_name>Startup</jsScript_name>
        <jsScript_script>var customChartTitleMap;

var filePath = getVariable("Internal.Job.Filename.Directory", "") + "/configCustomChartDescription.csv";

// Don't know why, but fileExists does not always work, even though loadFileContent does...
// So try catch is the only safe way...
//if(fileExists(filePath)) {
try{
    var data = loadFileContent(filePath);
    data.split(/[\r\n]+/).forEach(function(line, i){
	    if(i > 0) { // first line is header
    	    var nameAndTitle = line.split(";");
        	if(!customChartTitleMap){ customChartTitleMap = {}; }
	    	customChartTitleMap[nameAndTitle[0]] = nameAndTitle[1];
        }
    });
} catch(ex) {
   // Swallow
   customChartTitleMap = null;
}
//}
</jsScript_script>
      </jsScript>
    </jsScripts>
