// propertyOutputType
var propertyOutputType = simpleType.charAt(0).toUpperCase() + simpleType.substr(1);
if(propertyOutputType === 'List'){
    propertyOutputType = 'Array';
}

// propertyInputType
var propertyInputType;
if(valuesAndLabels &amp;&amp; valuesAndLabels.length) {
    propertyInputType = "";
} else if(propertyOutputType === 'String') {
	propertyInputType = 'String';
} else if(propertyOutputType === 'Function') {
	propertyInputType = 'JavaScript';
} else if (propertyOutputType === 'Number') {
	propertyInputType = 'Float';
} else {
    propertyInputType = propertyOutputType;
}

// cdePropertyName
var cdePropertyName = 'ccc' + prettyName.charAt(0).toUpperCase() + prettyName.substr(1);

// cdePropertyLabel
var cdePropertyLabel = customPropDescMap[prettyName];
if(cdePropertyLabel == null) {
  // Convert prettyName to a title
  cdePropertyLabel = prettyName.replace(/([a-z\d])([A-Z])/g, "$1 $2");

  cdePropertyLabel = cdePropertyLabel.charAt(0).toUpperCase() + cdePropertyLabel.substr(1);

  cdePropertyLabel = cdePropertyLabel.replace(/([a-z])(\d)/gi, "$1 $2");
}

var advanced = customPropAdvancedMap[prettyName];
if(advanced == null) 
  advanced = ''+(level != 'basic');
else
  advanced = '' + advanced;

var cdePropertyXml = &lt;DesignerProperty>	
     &lt;Header>
    	&lt;Name>{cdePropertyName}&lt;/Name>
        &lt;Parent>BaseProperty&lt;/Parent>
        &lt;DefaultValue>&lt;/DefaultValue>
        &lt;Description>{cdePropertyLabel}&lt;/Description>
        &lt;Tooltip>{documentation}&lt;/Tooltip>
        &lt;InputType>{propertyInputType}&lt;/InputType>
        &lt;OutputType>{propertyOutputType}&lt;/OutputType>
        &lt;Order>{order}&lt;/Order>
        &lt;Advanced>{advanced}&lt;/Advanced>
        &lt;Version>2.0&lt;/Version>
    &lt;/Header>
&lt;/DesignerProperty>;

cdePropertyXml.Header.DefaultValue = defaultValue; // Don't strip spaces

if(valuesAndLabels &amp;&amp; valuesAndLabels.length) {
    var inputType = &lt;InputType type="valuelist"/>;
	cdePropertyXml.Header.InputType = inputType;
     
    var valuesXml = &lt;Values>&lt;/Values>;
    valuesAndLabels.sort(function(a, b){ return a.label &lt; b.label ? -1 : a.label > b.label ? 1 : 0 });
    valuesAndLabels.forEach(function(valueAndLabel){
        var value = valueAndLabel.value;
        var label = valueAndLabel.label;
        valuesXml.appendChild(&lt;Value display={label}>{value}&lt;/Value>);
    });
    cdePropertyXml.appendChild(valuesXml);
}

cdePropertyXml = cdePropertyXml.toXMLString();</jsScript_script>
